<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://topdown.soacourse.unsw.edu.au" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="TopDownSimpleService" targetNamespace="http://topdown.soacourse.unsw.edu.au">
  <wsdl:types>
    <xsd:schema targetNamespace="http://topdown.soacourse.unsw.edu.au">
       <xsd:element name="importMarketDataRequest">
      	<xsd:complexType>
          <xsd:sequence>
            <xsd:element name="sec" nillable="false" type="xsd:string"/>
            <xsd:element name="startDate" nillable="false" type="xsd:string"/>
            <xsd:element name="endDate" nillable="false" type="xsd:string"/>
            <xsd:element name="dataSource" nillable="false" type="xsd:string"/>
          </xsd:sequence>
        </xsd:complexType>
       </xsd:element>
       <xsd:element name="importMarketDataResponse">
        <xsd:complexType>
           <xsd:sequence>
               <xsd:element name="returnData" nillable="false" type="xsd:string"/>
           </xsd:sequence>
        </xsd:complexType>
       </xsd:element>
       <xsd:element name="downloadFileRequest">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetID" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="downloadFileResponse">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="dataURL" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="summariseMarketDataRequest">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetID" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="summariseMarketDataResponse">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetId" nillable="false" type="xsd:string"/>
             <xsd:element name="sec" nillable="false" type="xsd:string"/>
             <xsd:element name="startDate" nillable="false" type="xsd:string"/>
             <xsd:element name="endDate" nillable="false" type="xsd:string"/>
             <xsd:element name="currencyCode" nillable="false" type="xsd:string"/>
             <xsd:element name="fileSize" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="convertMarketDataRequest">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetID" nillable="false" type="xsd:string"/>
             <xsd:element name="targetCurrency" nillable="false" type="xsd:string"/>
             <xsd:element name="targetDate" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="convertMarketDataResponse">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetIf" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="visualiseMarketDataRequest">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="eventSetID" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:element name="visualiseMarketDataResponse">
        <xsd:complexType>
         <xsd:sequence>
             <xsd:element name="dataURL" nillable="false" type="xsd:string"/>
         </xsd:sequence>
        </xsd:complexType>
      </xsd:element>
      <xsd:complexType name="serviceFaultType">
      	<xsd:sequence>
      		<xsd:element name="errcode" type="xsd:string"></xsd:element>
      		<xsd:element name="errtext" type="xsd:string"></xsd:element>
      	</xsd:sequence>
      </xsd:complexType>
      <xsd:element name="importMarketFault"    type="tns:serviceFaultType"></xsd:element>
      <xsd:element name="downloadFileFault"    type="tns:serviceFaultType"></xsd:element>
      <xsd:element name="summariseMarketFault" type="tns:serviceFaultType"></xsd:element>
      <xsd:element name="convertMarketFault"   type="tns:serviceFaultType"></xsd:element>
      <xsd:element name="visualiseMarketFault" type="tns:serviceFaultType"></xsd:element>
    </xsd:schema>
  </wsdl:types>
  <wsdl:message name="importMarketDataRequestMsg">
    <wsdl:part name="parameters" element="tns:importMarketDataRequest" />
  </wsdl:message>
  <wsdl:message name="importMarketDataResponseMsg">
    <wsdl:part name="parameters" element="tns:importMarketDataResponse" />
  </wsdl:message>
  <wsdl:message name="importMarketFaultMsg">
